{
  "expo": {
    "name": "eBankingFront",
    "slug": "eBankingFront",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ebankingfront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.ebankingfront" // Add a unique bundle ID if not present
    },
    "notification": {
      "icon": "./assets/Icons/Notification.png",
      "color": "#000000",
      "iosDisplayInForeground": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.yourcompany.ebankingfront", // Add a unique package name if not present
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e1272128-34b3-4a19-afac-73647d61898a" 
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}